<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="ProgressDotView" tools:ignore="ResourceName">
        <attr name="dotCount" format="integer"/>
        <attr name="unSelectedColor" format="color"/>
        <attr name="selectedColor" format="color"/>
        <attr name="dotRadius" format="float"/>
        <attr name="circleZoom" format="float"/>
        <attr name="leftCircleCanMovePosition" format="float"/>
    </declare-styleable>

    <declare-styleable name="AppearLayout" tools:ignore="ResourceName">
        <!--背景颜色-->
        <attr name="bgColor" format="color"/>
    </declare-styleable>
</resources>